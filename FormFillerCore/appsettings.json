{
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com/",
    "Domain": "[azure domain]",
    "TenantId": "[azure tenantid]",
    "ClientId": "[azure app registration client id]",
    "CallbackPath": "/signin-oidc",
    "Scopes": "Default"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "KeyVaultData": {
    "KeyVaultName": "[azure key vault name]",
    "AzureADAppID": "[azure ad app id of app accessing key vault]",
    "AzureADThumbprint": "[Cert Thumbprint]",
    "AzureADDirectoryID": "[Azure AAD guid]"
  },
  "BuildUpdateCheck": {
    "Current Build": "With Cors focused on powerautomate"
  },
  "AllowedHosts": "*"
}
